handlers:
- url: /img/(.+)
  static_files: static/img/\1
  upload: static/img/(.+)

- url: /css/(.+)
  static_files: static/css/\1
  upload: static/css/(.+)

- url: /js/(.+)
  static_files: static/js/\1
  upload: static/js/(.+)

- url: /(.+)
  mime_type: text/html
  static_files: static/\1
  upload: static/(.+)

- url: /favicon\.ico
  static_files: static/img/favicon.ico
  upload: static/img/favicon\.ico
