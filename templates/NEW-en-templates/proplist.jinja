<!DOCTYPE html>
<html>
<head>
<meta content="text/html; charset=utf-8" http-equiv="Content-Type" />
<title>{{title}}</title>
<meta content="{{keywords}}" name="keywords" />
<meta content="{{description}}" name="description" />
<meta content="en" http-equiv="Content-Language" />
<meta content="IE=edge" http-equiv="X-UA-Compatible" />
<meta content="width=device-width, initial-scale=1.0" name="viewport">
<link rel="canonical" href="{{enurl}}{{url_en}}" />
<link rel="alternate" hreflang="en" href="{{enurl}}{{url_en}}" />
<link rel="alternate" hreflang="de" href="{{deurl}}{{url_de}}" />
<link rel="alternate" hreflang="fr" href="{{frurl}}{{url_fr}}" />
<link rel="alternate" hreflang="nl" href="{{nlurl}}{{url_nl}}" />
<script src="https://kit.fontawesome.com/73fe5491f6.js" crossorigin="anonymous"></script>
<link rel="stylesheet" href="{{enurl}}css/mgh.css?26">
<link rel="shortcut icon" href="/img/favicon.ico" />
</head>
<body  class="has-navbar-fixed-top">
<header>
{% include "nav.html" %}
<script>(function(){var burger=document.querySelector('.burger');var nav=document.querySelector('#'+burger.dataset.target);burger.addEventListener('click',function(){burger.classList.toggle('is-active');nav.classList.toggle('is-active');});})();</script>
</header>
<h1 class="is-size-3 is-size-5-mobile has-text-centered">{{title}}</h1>
<!-- START PROPERTY -->
      <div class="columns is-mobile is-multiline " id="rubrun">
          <!-- 1 -->
          {% for item in props %}
          <div class="column is-2-desktop is-12-mobile is-4-tablet has-text-centered">
            <div class="card">
            <a href="{{ item['propurl'] }}">
              <div class=card-content>
                <div class="card-image">
                  <figure class="image is-4by3">
                      
                          <img src="{{item['img']|replace("w960", "w300-c-h220")}}" alt="{{item['locationdetail']}} {{item['proptype']}} for {{item['saleorrent']}}" title="{{item['locationdetail']}} {{item['proptype']}} for {{item['saleorrent']}}">                      
                  </figure>
              </div>

                  <div class="media">
                        <div class="media-content">
                          <p class="is-size-6 has-text-centered xhas-text-weight-semibold is-uppercase">{{item['proptype']}}</p>
                          <p class="is-size-7 has-text-centered">{{item['locationdetail']}}</p>
                          <p class="is-size-7 has-text-centered"> ref: {{item['ref']}} </p>
                          <!--<p class="is-size-7 has-text-centered">
                            <span><i class="fa fa-bed fa-lg" aria-hidden=true></i> {{item['beds']}} </span>
                            <span><i class="fa fa-bath fa-lg" aria-hidden=true></i> {{item['baths']}} </span>
                          </p> -->
                        </div>
                  </div>
                </a>
                  
                  <footer class="card-footer">
                      <p class=card-footer-item>
                          <span class=" is-size-7 is-size-6-mobile">
                            <i class="fa fa-bed fa-lg" aria-hidden=true></i> {{item['beds']}}
                          </span>

                      </p>
                      <p class=card-footer-item>
                          <span class=" is-size-7 is-size-6-mobile">
                            <i class="fa fa-bath fa-lg" aria-hidden=true></i> {{item['baths']}}
                          </span>
                      </p>
                      <p class=card-footer-item>
                          <span class=" is-size-7 is-size-6-mobile">
                            <i class="fa fa-swimming-pool fa-lg" aria-hidden=true></i> {{item['haspool']}}
                          </span>
                      </p>
                  </footer>
                  <footer class=card-footer>
                      <p class="card-footer-item price">
                          <span class="is-size-6 is-size-5-mobile has-text-weight-bold"> {{item['price']}} </span>                          
                      </p>
                  </footer>                 
              </div>
           </div>
          </div>
          {% endfor %}
      </div>
  <!-- END PROPERTY -->
 {% include "footer.html" %}
