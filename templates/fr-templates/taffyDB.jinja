<props>
{% for item in props %}<property>
<id>{{item['propid']}}</id>
<date>{{item['date']}}</date>
<ref>{{item['propref']}}</ref>
<price>{{item['price']}}</price>
<price_freq>{{item['frequency']}}</price_freq>
<type>{{item['proptype']}}</type>
<town>{{item['town']}}</town>
<province>{{item['province']}}</province>
<location_detail>{{item['locationdetail']}}</location_detail>
<beds>{{item['beds']}}</beds>
<baths>{{item['baths']}}</baths>
<pool>{{item['pool']}}</pool>
<url>{{item['propurl']}}</url>
<desc>{{item['description']}}</desc>
<thumb>{{item['img']|replace("s640-rj-e30-v1","s400-rj-e30-v1?.jpg")|replace("s640-rj-e30-v2","s400-rj-e30-v2?.jpg")}}</thumb>
</property>{% endfor %}
</props>

